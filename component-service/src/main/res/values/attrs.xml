<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SocialToolbar">
        <attr name="toolbar_left_icon" format="reference" />
        <attr name="toolbar_right_icon" format="reference" />
        <attr name="toolbar_right_text" format="string" />
        <attr name="toolbar_right_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="AutoFitGridLayout">
        <attr name="columnCount" format="integer" />
        <attr name="horizontalSpace" format="dimension" />
        <attr name="verticalSpace" format="dimension" />
    </declare-styleable>
</resources>