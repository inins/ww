<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FunshowView">
        <!-- 是否显示header -->
        <attr name="show_header" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BannerView">
        <attr name="selected_color" format="color|reference" />
        <attr name="unselected_color" format="color|reference" />
        <attr name="autoscroll" format="boolean" />
        <attr name="number_indicator" format="boolean" />
        <attr name="is_square">
            <enum name="by_width" value="1" />
            <enum name="by_hight" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BundleImgView">
        <!-- 是否可编辑状态 -->
        <attr name="editble" format="boolean" />
        <!-- 是否可拖拽手动排序 -->
        <attr name="dragble" format="boolean" />
        <attr name="space_width" format="dimension" />
        <attr name="wihi" format="float" />
        <attr name="corner" format="dimension" />
        <attr name="colcount" format="integer" />
        <attr name="addbtnlayout" format="reference" />
    </declare-styleable>

    <declare-styleable name="TitleView">
        <attr name="personal_textTitle" format="string" />
        <attr name="personal_textSubTitle" format="string" />
        <attr name="personal_textNote" format="string" />
    </declare-styleable>

    <declare-styleable name="BarView">
        <attr name="title" format="string" />
        <attr name="note" format="string" />
        <attr name="max_count" format="integer" />
    </declare-styleable>

    <declare-styleable name="LoadingLayout">
        <attr name="layout_loading" format="reference" />
        <attr name="layout_lack" format="reference" />
        <attr name="layout_fail" format="reference" />
    </declare-styleable>

    <declare-styleable name="SocialToolbar">
        <attr name="toolbar_left_icon" format="reference" />
        <attr name="toolbar_right_icon" format="reference" />
        <attr name="toolbar_right_text" format="string" />
        <attr name="toolbar_right_text_color" format="color" />
        <attr name="toolbar_title_text" format="string" />
        <attr name="toolbar_title_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="AutoFitGridLayout">
        <attr name="columnCount" format="integer" />
        <attr name="horizontalSpace" format="dimension" />
        <attr name="verticalSpace" format="dimension" />
    </declare-styleable>

    <attr name="halfVisibleItemCount" format="integer" />
    <attr name="itemTextSize" format="dimension" />
    <attr name="itemMaximumWidthText" format="string" />
    <attr name="itemTextColor" format="color" />
    <attr name="textGradual" format="boolean" />
    <attr name="selectedTextColor" format="color" />
    <attr name="selectedTextSize" format="dimension" />
    <attr name="currentItemPosition" format="integer" />
    <attr name="itemWidthSpace" format="dimension" />
    <attr name="itemHeightSpace" format="dimension" />
    <attr name="zoomInSelectedItem" format="boolean" />
    <attr name="wheelCyclic" format="boolean" />
    <attr name="wheelCurtain" format="boolean" />
    <attr name="wheelCurtainColor" format="color" />
    <attr name="wheelCurtainBorder" format="boolean" />
    <attr name="wheelCurtainBorderHorizontal" format="boolean" />
    <attr name="wheelCurtainBorderColor" format="color" />
    <attr name="indicatorText" format="string" />
    <attr name="indicatorTextSize" format="dimension" />
    <attr name="indicatorTextColor" format="color" />
    <declare-styleable name="WheelPicker">
        <attr name="halfVisibleItemCount" />
        <attr name="itemTextSize" />
        <attr name="itemMaximumWidthText" />
        <attr name="itemTextColor" />
        <attr name="textGradual" />
        <attr name="selectedTextColor" />
        <attr name="selectedTextSize" />
        <attr name="currentItemPosition" />
        <attr name="itemWidthSpace" />
        <attr name="itemHeightSpace" />
        <attr name="zoomInSelectedItem" />
        <attr name="wheelCyclic" />
        <attr name="wheelCurtain" />
        <attr name="wheelCurtainColor" />
        <attr name="wheelCurtainBorder" />
        <attr name="wheelCurtainBorderHorizontal" />
        <attr name="wheelCurtainBorderColor" />
        <attr name="indicatorText" />
        <attr name="indicatorTextSize" />
        <attr name="indicatorTextColor" />
    </declare-styleable>

    <declare-styleable name="YearPicker">
        <attr name="startYear" format="integer" />
        <attr name="endYear" format="integer" />
    </declare-styleable>

    <declare-styleable name="MonthPicker"></declare-styleable>

    <declare-styleable name="DatePicker">
        <attr name="halfVisibleItemCount" />
        <attr name="itemTextSize" />
        <attr name="itemTextColor" />
        <attr name="textGradual" />
        <attr name="selectedTextColor" />
        <attr name="selectedTextSize" />
        <attr name="itemWidthSpace" />
        <attr name="itemHeightSpace" />
        <attr name="zoomInSelectedItem" />
        <attr name="wheelCyclic" />
        <attr name="wheelCurtain" />
        <attr name="wheelCurtainColor" />
        <attr name="wheelCurtainBorder" />
        <attr name="wheelCurtainBorderHorizontal" />
        <attr name="wheelCurtainBorderColor" />
    </declare-styleable>
</resources>